<ul>
    {{#each collections}}
      <li>
        This is the title: {{this.title}}  by {{ this.author.firstname }}
        <img src ={{this.collectionPhotoUrl}} alt="image of collection">
        <a href="/collection/{{this._id}}">Click to see photos in this collection"</a>
        
      </li>
    {{/each}}
  </ul>
{{else}}
  <p>No collections!</p>
{{/if}}